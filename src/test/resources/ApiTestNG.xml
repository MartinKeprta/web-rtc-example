<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Api tests">
    <test name="Status">
        <classes>
            <class name="com.testrtc.api.StatusTest"/>
        </classes>
    </test>

    <test name="Launch tests">
        <classes>
            <class name="com.testrtc.api.TestRun"/>
        </classes>
    </test>

    <test name="Test information">
        <classes>
            <class name="com.testrtc.api.TestInfo"/>
        </classes>
    </test>

    <test name="Test result">
        <classes>
            <class name="com.testrtc.api.TestResult"/>
        </classes>
    </test>


</suite>